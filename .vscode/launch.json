	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "npm run dev:electron",
			"runtimeExecutable": "npm",
			"runtimeArgs": [
				"run",
				"dev:electron"
			],
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "electron dist/main/electron.js",
			"runtimeExecutable": "electron",
			"program": "${workspaceFolder}/dist/main/electron.js",
			"cwd": "${workspaceFolder}"
		},


	]
}